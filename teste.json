{
    "quiet": false,
    "repositorys": [
        {
            "repository": "../SiteOui/",
            "ignore": false,
            "comands": [
                "npm install  --prefix ../SiteOui/ && npm run build  --prefix ../SiteOui/ && npm start  --prefix ../SiteOui/"
            ],
            "timewait": 10
        },
        {
            "repository": "../OiDemoAbc/",
            "comands": [
                "flask --app ../OiDemoAbc/main.py run --port 5000",
                "flask --app ../OiDemoAbc/main.py run --port 5001"
            ],
            "timewait": 10
        },
        {
            "repository": "../OiDemoTemplates/",
            "comands": [
                "python3 sync_templates.py"
            ],
            "timewait": 10
        }
    ]
}